<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /wfo/api/
    RewriteRule ^index\.php$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /wfo/api/index.php [L]
</IfModule>

<Files "log.txt">
    Order Allow,Deny
    Deny from all
</Files>